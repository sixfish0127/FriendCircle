    <script src="~/js/jquery.easypiechart.min.js"></script>
    <script>
        $('.chart').easyPieChart({
            easing: 'easeOutElastic',
            delay: 3000,
            barColor: '#3498db',
            trackColor: '#aaa',
            scaleColor: false,
            lineWidth: 5,
            trackWidth: 5,
            size: 50,
            lineCap: 'round',
            onStep: function (from, to, percent) {
                this.el.children[0].innerHTML = Math.round(percent);
            }
        });

        //載入通知
        document.addEventListener("DOMContentLoaded", function () {
            loadNotifications();
            loadFriends();
        });
        //切換聊天室窗
        function toggleblock(){
            const block = document.getElementById('chat-container');
            if(block.className==='modal-popup-chat')
            {block.className='modal-popup-chat d-block';}else{
                block.className='modal-popup-chat';
            }
        }
        function toggleChatWindow(friendName, friendId,friendstatus, friendimage) {
            toggleblock()
            console.log("已觸發方法");
            // 確認聊天室是否已經存在
            const existingChatWindow = document.getElementById(`chat-window-${friendId}`);
            if (existingChatWindow) {
                  // 如果聊天室存在，隱藏或移除
                  existingChatWindow.remove();
            }else {
                  // 如果聊天室不存在，動態生成窗口
                  const chatWindow = document.createElement("div");
                  chatWindow.className = "modal-popup-wrap bg-white p-0 shadow-lg rounded-3";
                  chatWindow.id = `chat-window-${friendId}`;
                  chatWindow.innerHTML = `                    
                    <div class="modal-popup-header w-100 border-bottom">
                        <div class="card p-3 d-block border-0 d-block">
                            <figure class="avatar mb-0 float-left me-2">
                                <img src="${friendimage}" alt="image" class="w35 me-1">
                            </figure>
                            <h5 class="fw-700 text-primary font-xssss mt-1 mb-1">${friendName}</h5>
                            <h4 class="text-grey-500 font-xsssss mt-0 mb-0"><span class="d-inline-block bg-success btn-round-xss m-0"></span> ${friendstatus}</h4>                            
                        </div>
                    </div>
                    <div class="modal-popup-body w-100 p-3 h-auto">
                        <div class="message"><div class="message-content font-xssss lh-24 fw-500">Hi, how can I help you?</div></div>
                        <div class="date-break font-xsssss lh-24 fw-500 text-grey-500 mt-2 mb-2">Mon 10:20am</div>
                        <div class="message self text-right mt-2"><div class="message-content font-xssss lh-24 fw-500">I want those files for you. I want you to send 1 PDF and 1 image file.</div></div>
                        <div class="snippet pt-3 ps-4 pb-2 pe-3 mt-2 bg-grey rounded-xl float-right" data-title=".dot-typing"><div class="stage"><div class="dot-typing"></div></div></div>
                        <div class="clearfix"></div>
                    </div>
                    <div class="modal-popup-footer w-100 border-top">
                        <div class="card p-3 d-block border-0 d-block">
                            <div class="form-group icon-right-input style1-input mb-0"><input type="text" placeholder="Start typing.." class="form-control rounded-xl bg-greylight border-0 font-xssss fw-500 ps-3"><i class="feather-send text-grey-500 font-md"></i></div>
                        </div>
                    </div>                    
                  `;

                  // 將聊天室添加到容器
                  document.getElementById("chat-container").appendChild(chatWindow);
            }
        }
        
        //載入朋友清單
		function loadFriends() {
			const friendcontainer = document.getElementById('friend-container');
			fetch(`/api/friends/list`) // 呼叫後端 API
				.then(response => response.json())
				.then(friends => {
                    console.log("朋友資料:",friends);
					friendcontainer.innerHTML = ""; // 清空舊的內容
					if (friends.length > 0) {
						friends.forEach(f => {
							const friendUl = document.createElement('ul');
                            friendUl.className = "list-group list-group-flush";
                            if(f.status === 0||f.status === 4){
                                friendUl.innerHTML = `
                                <li class="bg-transparent list-group-item no-icon pe-0 ps-0 pt-2 pb-2 border-0 d-flex align-items-center">
                                    <figure class="avatar float-left mb-0 me-2">
                                        <img src="${f.userimage}" alt="image" class="w35">
                                    </figure>
                                    <h3 class="fw-700 mb-0 mt-0">
                                        <a class="font-xssss text-grey-600 d-block text-dark model-popup-chat" href="#" onclick="toggleChatWindow('${f.name}', ${f.id},${f.status}, '${f.userimage}');return false;">${f.name}</a>
                                    </h3>
                                    <span class="bg-secondary ms-auto btn-round-xss"></span>
                                </li>
                            `;
                            }else if(f.status === 1){
                                friendUl.innerHTML =`
                                    <li class="bg-transparent list-group-item no-icon pe-0 ps-0 pt-2 pb-2 border-0 d-flex align-items-center">
                                        <figure class="avatar float-left mb-0 me-2">
                                            <img src="${f.userimage}" alt="image" class="w35">
                                        </figure>
                                        <h3 class="fw-700 mb-0 mt-0">
                                            <a class="font-xssss text-grey-600 d-block text-dark model-popup-chat" id="alink" href="#" onclick="toggleChatWindow('${f.name}', ${f.id},${f.status}, '${f.userimage}'); return false;">${f.name}</a>
                                        </h3>
                                        <span class="bg-success ms-auto btn-round-xss"></span>
                                    </li>
                                `;
                            }else if(f.status === 2){
                                friendUl.innerHTML =`
                                    <li class="bg-transparent list-group-item no-icon pe-0 ps-0 pt-2 pb-2 border-0 d-flex align-items-center">
                                        <figure class="avatar float-left mb-0 me-2">
                                            <img src="${f.userimage}" alt="image" class="w35">
                                        </figure>
                                        <h3 class="fw-700 mb-0 mt-0">
                                            <a class="font-xssss text-grey-600 d-block text-dark model-popup-chat" href="#">${f.name}</a>
                                        </h3>
                                        <span class="bg-warning ms-auto btn-round-xss"></span>
                                    </li>
                                `;
                            }else if(f.status === 3){
                                friendUl.innerHTML =`
                                    <li class="bg-transparent list-group-item no-icon pe-0 ps-0 pt-2 pb-2 border-0 d-flex align-items-center">
                                        <figure class="avatar float-left mb-0 me-2">
                                            <img src="${f.userimage}" alt="image" class="w35">
                                        </figure>
                                        <h3 class="fw-700 mb-0 mt-0">
                                            <a class="font-xssss text-grey-600 d-block text-dark model-popup-chat" href="#" onclick="toggleChatWindow('${f.name}', ${f.id},${f.status}, '${f.userimage}'); return false;">${f.name}</a>
                                        </h3>
                                        <span class="bg-info ms-auto btn-round-xss"></span>
                                    </li>
                                `;
                            }else if(f.status === 5){
                                friendUl.innerHTML =`
                                    <li class="bg-transparent list-group-item no-icon pe-0 ps-0 pt-2 pb-2 border-0 d-flex align-items-center">
                                        <figure class="avatar float-left mb-0 me-2">
                                            <img src="${f.userimage}" alt="image" class="w35">
                                        </figure>
                                        <h3 class="fw-700 mb-0 mt-0">
                                            <a class="font-xssss text-grey-600 d-block text-dark model-popup-chat" href="#" onclick="toggleChatWindow('${f.name}', ${f.id},${f.status}, '${f.userimage}'); return false;">${f.name}</a>
                                        </h3>
                                        <span class="bg-danger ms-auto btn-round-xss"></span>
                                    </li>
                                `;
                            }
                            
                            friendcontainer.appendChild(friendUl);
						});
					} else {
						friendcontainer.innerHTML = "<li>沒有朋友</li>";
					}
				})
				.catch(error => {
					console.error('Error fetching friends:', error);
					document.getElementById('friend-container').innerHTML = "<li>載入出錯</li>";
				});
		}
		// 載入通知
        function loadNotifications() {
            const notificationcontainer = document.getElementById('notification-container');            
            fetch(`/api/notifications`) // 呼叫後端 API
                .then(response => response.json())
                .then(notifications => {                                        
                    notificationcontainer.innerHTML = ""; // 清空舊的內容

                    if (notifications.length > 0) {
                        notifications.forEach(n => {
                            console.log("n的資料:",n)
                            const relativeTime = calculateRelativeTime(n.createdAt);//計算時間
                            const notificationDiv = document.createElement('div');
                            notificationDiv.className=`card bg-transparent-card w-100 border-0 ps-5 mb-3 ${n.isRead ? 'bg-light' : 'bg-yellow'}`;
                            // 根據通知類型動態生成內容
                            if(n.type === 2){
                                notificationDiv.innerHTML = `
                                <img src="${n.commenterImage}" alt="user" class="w40 position-absolute left-0">
                                <h5 class="font-xsss text-grey-900 mb-1 mt-0 fw-700 d-block">${n.commenterName} <span class="text-grey-400 font-xsssss fw-600 float-right mt-1"> ${relativeTime}</span></h5>
                                <h6 class="fw-500 font-xssss lh-4">${n.message}</h6>
                            `;
                            }else if(n.type === 3){
                                if(!n.isRead){
                                    notificationDiv.innerHTML += `
                                    <img src="${n.friendImage || 'default-image.jpg'}" alt="user" class="w40 position-absolute left-0">
                                    <h5 class="font-xsss text-grey-900 mb-1 mt-0 fw-700 d-block">${n.friendName} <span class="text-grey-400 font-xsssss fw-600 float-right mt-1">${relativeTime}</span></h5>
                                    <h6 class="fw-500 font-xssss lh-4">${n.message}</h6>
                                    <div class="d-flex justify-content-start mt-2">
                                        <button class="btn btn-sm btn-success me-2" onclick="acceptFriendRequest(${n.friendId},'Accepted')">接受</button>
                                        <button class="btn btn-sm btn-danger" onclick="rejectFriendRequest(${n.friendId},'Rejected')">拒絕</button>
                                    </div>
                                `;
                                }                                
                            }else if(n.type === 4){
                                notificationDiv.innerHTML = `
                                <img src="${n.friendImage || 'default-image.jpg'}" alt="user" class="w40 position-absolute left-0">
                                <h5 class="font-xsss text-grey-900 mb-1 mt-0 fw-700 d-block">${n.friendName} <span class="text-grey-400 font-xsssss fw-600 float-right mt-1">${relativeTime}</span></h5>
                                <h6 class="fw-500 font-xssss lh-4">${n.message}</h6>
                                `;
                            }
                            
                            // 添加點擊事件
                            notificationDiv.onclick = () => markNotificationAsRead(n.id);
                            notificationcontainer.appendChild(notificationDiv);
                        });
                    } else {
                        notificationcontainer.innerHTML = "<li>沒有通知</li>";
                    }                    
                })
                .catch(error => {
                    console.error('Error fetching notifications:', error);
                    document.getElementById('notification-container').innerHTML = "<li>載入出錯</li>";
                });
        }
        //處理好友邀請
		function acceptFriendRequest(friendId, status){
			console.log(`接受好友請求: ${friendId}`);
            fetch(`/api/friends/${friendId}/response-request/${status}`, {
				method: "PUT",
				headers: {
					"Content-Type": "application/json",
				},				
			})
			.then(response => {
				if (response.ok) {
					console.log("好友請求已處理");
					// 可以選擇重新加載通知列表
					loadNotifications();
				} else {
					console.error("接受好友請求失敗");
				}
			})
			.catch(error => console.error("API 錯誤:", error));
		}

		function rejectFriendRequest(friendId, status){
            console.log(`拒絕好友請求: ${friendId}`);
            fetch(`/api/friends/${friendId}/response-request/${status}`, {
                method: "PUT",
                headers: {
                    "Content-Type": "application/json",
                },
                body: JSON.stringify({ status }),
            })
            .then(response => {
                if (response.ok) {
                    console.log("好友請求已接受");
                    // 可以選擇重新加載通知列表
                    loadNotifications();
                } else {
                    console.error("接受好友請求失敗");
                }
            })
        }
        function markNotificationAsRead(notificationId){
            console.log(`標記通知為已讀: ${notificationId}`);
            fetch(`/api/notifications/${notificationId}/mark-read`, {
                method: "PUT",
                headers: {
                    "Content-Type": "application/json",
                },
            })
            .then(response => {
            if (response.ok) {
                console.log("通知已標記為已讀");
                // 可以選擇重新加載通知列表
                loadNotifications();
            } else {
                console.error("標記通知為已讀失敗");
            }
        })
        .catch(error => console.error("API 錯誤:", error));
        }

        //計算相對時間
        function calculateRelativeTime(postTime) {
            const now = new Date();
            const postDate = new Date(postTime); // 將 ISO 時間轉為 Date 物件
            const diffMs = now.getTime() - postDate.getTime(); // 毫秒差異
            const diffSec = Math.floor(diffMs / 1000); // 秒
            const diffMin = Math.floor(diffSec / 60); // 分鐘
            const diffHour = Math.floor(diffMin / 60); // 小時
            const diffDay = Math.floor(diffHour / 24); // 天

            if (diffSec < 60) return "剛剛";
            if (diffMin < 60) return `${diffMin} 分鐘前`;
            if (diffHour < 24) return `${diffHour} 小時前`;
            return `${diffDay} 天前`;
        }

    </script>

